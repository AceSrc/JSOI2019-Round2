ret = [["测试点","$\sum_{i=1}^m k_i$", "$m$","$k_i$"]]
for datum in prob['args']:	#引用conf.json/yaml中的变量
	row = [
		','.join(map(str, datum['cases'])),	
		"$\le %s$" % tl.hn(datum['n']),
		r"$\le %s$" % tl.hn(datum['m']),
